{
  "name": "connectlist-minimal",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality": "npm run lint && npm run format:check && npm run type-check",
    "analyze": "npx webpack-bundle-analyzer dist/static/js/*.js",
    "build:analyze": "expo export:web && npm run analyze",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:all": "eas submit --platform all",
    "preview:ios": "eas build --platform ios --profile preview",
    "preview:android": "eas build --platform android --profile preview",
    "store:prepare": "node scripts/prepare-store-assets.js",
    "store:validate": "node scripts/validate-store-metadata.js",
    "screenshots:capture": "node scripts/capture-screenshots.js",
    "deploy:production": "npm run quality && npm run build:all && npm run submit:all"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@supabase/supabase-js": "^2.56.0",
    "@sentry/react-native": "^5.33.1",
    "isomorphic-dompurify": "^2.16.0",
    "expo": "~53.0.20",
    "expo-constants": "^17.1.7",
    "expo-device": "^7.1.4",
    "expo-font": "^13.3.2",
    "expo-haptics": "^14.1.4",
    "expo-image-picker": "~16.1.4",
    "expo-notifications": "^0.31.4",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.15.2",
    "react-native-svg": "^15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "typescript": "~5.8.3",
    "@types/react": "~19.0.10",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@react-native/eslint-config": "^0.75.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "prettier": "^3.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "private": true
}
