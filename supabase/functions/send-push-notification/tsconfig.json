{
  "compilerOptions": {
    "allowJs": true,
    "allowUmdGlobalAccess": true,
    "checkJs": false,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["deno.window"],
    "module": "esnext",
    "moduleDetection": "force",
    "noEmit": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "esnext",
    "useDefineForClassFields": true,
    "types": ["@supabase/functions-js"]
  }
}
